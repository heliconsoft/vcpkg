{
  "name": "libraw",
  "version": "0.20.2",
  "port-version": 0,
  "description": "raw image decoder library",
  "homepage": "https://www.libraw.org",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "zlib",
      "$comment": "DNG deflate codec support"
    },
    {
      "name": "libjpeg-turbo",
      "$comment": "DNG lossy codec support"
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "features": {
    "openmp": {
      "description": "OpenMP support"
    },
    "lcms": {
      "description": "LCMS support",
      "dependencies": [ "lcms" ]
    },
    "jasper": {
      "description": "RedCine codec support",
      "dependencies": [ "jasper" ]
    },
    "rawspeed": {
      "description": "RawSpeed codec support",
      "dependencies": [ "libxml2" ]
    },
    "x3ftools": {
      "description": "Foveon X3F support"
    },
    "6by9rpi": {
      "description": "Raspberry Pi RAW support"
    }
  }
}
